{
    "compact": false,
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": ">0.25%",
                "bugfixes": true,
                "useBuiltIns": "entry",
                corejs: 3
            }
        ],
        '@babel/preset-react'
    ],
    "sourceMaps": true,
    plugins: [
        '@babel/transform-runtime',
        [
            "@babel/plugin-proposal-decorators",
            {
                "legacy": true
            }
        ]
    ]
}